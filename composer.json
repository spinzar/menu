{
   "name":"spinzar/menu",
   "description":"Laravel package for menu & sidebar.",
   "keywords":["laravel", "menu", "navigation", "sidebar", "bootstrap"],
   "license":"MIT",
   "authors":[
		{  
			"name":"Nasibullah Nassim",
			"email":"info@spinzar.co",
			"homepage":"https://spinzar.co",
			"role":"Developer"
		}
	],
	"require": {
	  "php": ">=7.1",
	  "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
	  "illuminate/config": "^6.0",
	  "illuminate/view": "^6.0",
	  "laravelcollective/html": "6.0.*"
	},
	"require-dev": {
	  "phpunit/phpunit": "^8.0",
	  "mockery/mockery": "~1.0",
	  "orchestra/testbench": "^4.0",
	  "friendsofphp/php-cs-fixer": "^2.15"
	},
   "autoload":{  
		"psr-4":{  
			"Spinzar\\Menu\\":"./src"
		},
		"files":[  
			"src/helpers.php"
		]
   },
   "extra":{  
		"laravel":{  
			"providers":[  
				"Spinzar\\Menu\\Provider"
			],
			"aliases": {
				"Menu": "Spinzar\\Menu\\Facade"
			}
		}
   }
}
